.data_signal {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 30rpx;
}

.data_line {
  height: 90rpx;
}

.task_state {
  display: inline-block;
  width: 16rpx;
  height: 16rpx;
  border-radius: 8rpx;
}

.task_completed {
  background-color: #00d34a;
}

.task_wait {
  background-color: #ccc;
}

.task_updated {
  background-color: #444;
}

.task_processing {
  background-color: #fd7d3d;
}

.task_error {
  background-color: red;
}
