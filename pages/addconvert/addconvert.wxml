      <input type='text' placeholder='标识'></input>
    </view>
    <view class='setting_line_right'>
    </view>
  </view>
  <view class='setting_line'>
    <view class='setting_line_name'>
      <text>输出格式</text>
    </view>
    <view class='setting_line_right'>
      <view class='setting_line_data'>默认</view>
      <text class="iconfont icon-m-right"></text>
    </view>
  </view>
   <view class='setting_line'>
    <view class='setting_line_name'>
      <text>转换模式</text>
    </view>
