    </view>
  </view>
   <view class='setting_line'>
    <view class='setting_line_name'>
      <text>Left</text>
    </view>
    <view class='setting_line_right'>
      <view class='setting_line_data'>0</view>
      <text class="iconfont icon-m-right"></text>
    </view>
  </view>
   <view class='setting_line'>
    <view class='setting_line_name'>
      <text>宽度</text>
    </view>
    <view class='setting_line_right'>
      <view class='setting_line_data'>300px</view>
      <text class="iconfont icon-m-right"></text>
    </view>
  </view>
   <view class='setting_line'>
    <view class='setting_line_name'>
      <text>高度</text>
    </view>
    <view class='setting_line_right'>
      <view class='setting_line_data'>300px</view>
      <text class="iconfont icon-m-right"></text>
    </view>
  </view>
   <button class="buttom_primary" hover-class='buttom_primary_hover' bindtap='login'> 确 定 </button>
</view>