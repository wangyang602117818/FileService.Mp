.image_list {
  width: 100%;
  float: left;
}

.image_item {
  display: inline-flex;
  height: 200rpx;
  width: 210rpx;
  margin-right: 10rpx;
  margin-top: 10rpx;
  overflow: hidden;
  border: 1px solid #ccc;
  align-items: center;
  justify-content: center;
  vertical-align: top;
  position: relative;
}

.image_item image {
  width: 100%;
  height: 100%;
}

.background_item {
  position: absolute;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  display: flex;
  align-items: center;
  justify-content: center;
}

.background_del {
  height: 60rpx;
  width: 80%;
  border-radius: 6rpx;
  background: #fff;
  font-size: 25rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.icon-m-plus {
  font-size: 80rpx;
}

.convert_wrap, .access_wrap {
  width: 100%;
  margin-top: 40rpx;
}
