.input_line {
  font-size: 25rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
  margin-top: 25rpx;
}

.input_line_title {
  margin-top: 10rpx;
  flex: 1;
}

.input_line_value {
  flex: 1;
  margin-top: 15rpx;
}

.input_line_value input {
  border: 1px solid #ccc;
  padding: 3rpx 5rpx;
}
